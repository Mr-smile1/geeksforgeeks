

class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        String rev = "";
        char a[] = str.toCharArray();
        int len = str.length();
        
        for(int i=str.length()-1;i>=0;i--){
            rev = rev + a[i];
        }
        return rev;
    }
}

