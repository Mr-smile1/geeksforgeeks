class GfG
{
	public static int palinArray(int[] a, int n)
    {
        int count=0;
        String s = new String();
        String p = new String();
        for(int i=0 ; i<n ; i++){
            s = Integer.toString(a[i]);
            p = s.reverse();
            if(s == p){
                count++;
            }
        }
        if(count == n){
            return 1;
        }
        return 0;
    }
}
